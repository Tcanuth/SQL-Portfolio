--Select all from table
Select * from Inventory;

--Select all puppies and list name, breed, and price and order them by price
Select Name, Breed, PRICE FROM Inventory ORDER BY Price;

--Display total store potential inventory value
Select Sum(Price) AS "Value" FROM Inventory;

--List all puppy names
Select Name FROM Inventory

